{
  "name": "test-slickgrid",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4400 --open",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-demo": "npm-run-all build",
    "postbuild-demo": "npm-run-all del-ghpage copy-ghpage:dist",
    "copy-ghpage:dist": "cross-env copyfiles --up 1 dist-demo/**/*.* ../../Angular-Slickgrid-GhPages/",
    "copy:i18n": "cross-env copyfiles -f node_modules/angular-slickgrid/i18n/*.json src/assets/i18n",
    "del-ghpage": "cross-env rimraf ../../Angular-Slickgrid-GhPages/**/*.*"
  },
  "private": false,
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead",
    "IE 11"
  ],
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/common": "^8.2.12",
    "@angular/compiler": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/forms": "^8.2.12",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@angular/router": "^8.2.12",
    "@ng-select/ng-select": "^3.0.7",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "angular-slickgrid": "^2.16.5",
    "bootstrap": "^3.4.1",
    "copyfiles": "^2.1.0",
    "core-js": "^2.6.5",
