    "text-encoding-utf-8": "^1.0.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.15",
    "@angular/cli": "^8.2.12",
    "@angular/compiler-cli": "^8.2.12",
    "@angular/language-service": "^8.2.12",
    "@types/jasmine": "~3.3.9",
